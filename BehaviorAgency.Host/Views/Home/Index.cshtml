@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Behavior Agency Solutions</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @foreach (var linkTag in ViewBag.Links)
    {
        @Html.Raw(linkTag)
    }

    @foreach (var styleTag in ViewBag.Styles)
    {
        @Html.Raw(styleTag)
    }
</head>
<body>

    <behavior-app-root>Loading...</behavior-app-root>

    <div id="nb-global-spinner" class="spinner">
        <div class="blob blob-0"></div>
        <div class="blob blob-1"></div>
        <div class="blob blob-2"></div>
        <div class="blob blob-3"></div>
        <div class="blob blob-4"></div>
        <div class="blob blob-5"></div>
    </div>

    @foreach (var scriptTag in ViewBag.Scripts)
    {
        @Html.Raw(scriptTag)
    }

</body>
</html>